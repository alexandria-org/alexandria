    	background : {{stickybar.overall.background_color}};
        {% if stickybar.overall.position == 'position_top' %}
            top : {{stickybar.overall.offset}}px;
            bottom : unset ;
        {% endif %}
        {% if stickybar.overall.position == 'position_bottom' %}
            top : unset ;
            bottom : {{stickybar.overall.offset}}px;
        {% endif %}
    }
